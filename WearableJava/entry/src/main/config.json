{
  "app": {
    "bundleName": "com.huawei.wearablejava",
    "vendor": "huawei",
    "version": {
      "code": 1000000,
      "name": "1.0.0"
    },
    "apiVersion": {
      "compatible": 5,
      "target": 5,
      "releaseType": "Release"
    }
  },
  "deviceConfig": {},
  "module": {
    "package": "com.huawei.wearablejava",
    "name": ".MyApplication",
    "deviceType": [
      "wearable"
    ],
    "distro": {
      "deliveryWithInstall": true,
      "moduleName": "entry",
      "moduleType": "entry"
    },
    "abilities": [
      {
        "skills": [
          {
            "entities": [
              "entity.system.home"
            ],
            "actions": [
              "action.system.home"
            ]
          }
        ],
        "orientation": "landscape",
        "name": "com.huawei.wearablejava.MainAbility",
        "icon": "$media:icon",
        "description": "First Ability",
        "label": "$string:app_name",
        "type": "page",
        "launchType": "standard"
      },
      {
        "orientation": "landscape",
        "name": "com.huawei.wearablejava.SecondAbility",
        "icon": "$media:icon",
        "description": "Second Ability",
        "label": "$string:app_name",
        "type": "page",
        "launchType": "standard"
      }
    ]
  }
}